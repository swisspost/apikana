<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>{{ dotnetPackageId }}</PackageId>
    <Namespace>{{ dotnetNamespace }}</Namespace>
    <!-- Custom Tag to specify the root namespace. -->
    <Authors>{{ author }}</Authors>
    <Company>{{ domain }}</Company>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <OutputType>Library</OutputType>

    <!--
            Version following Semantic Versioning guidelines: https://semver.org/
            If there is a suffix, the NuGet-Package will be treated as pre-release.
     -->
    <Version>1.0.0-rc.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <!-- Packages referenced by the classes generated by NSwag -->
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
  </ItemGroup>

</Project>